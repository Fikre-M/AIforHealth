{
  "name": "aiforhealth",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose",
    "test:component": "vitest run --reporter=verbose src/**/*.test.tsx",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:staging": "playwright test --config=playwright.staging.config.ts",
    "lighthouse": "lhci autorun",
    "lighthouse:desktop": "lighthouse http://localhost:4173 --preset=desktop --view",
    "lighthouse:mobile": "lighthouse http://localhost:4173 --preset=mobile --view",
    "perf:budget": "node scripts/check-performance-budget.js",
    "perf:analyze": "ANALYZE=true npm run build",
    "perf:report": "npm run build && npm run lighthouse"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.20.1",
    "@sentry/react": "^7.99.0",
    "@sentry/tracing": "^7.99.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@lhci/cli": "^0.12.0",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "lighthouse": "^11.4.0",
    "postcss": "^8.4.31",
    "rollup-plugin-visualizer": "^5.11.0",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^5.4.10",
    "vite-plugin-compression2": "^0.11.0"
  }
}
